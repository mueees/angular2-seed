{
  "name": "webpack-angular2",
  "version": "1.0.0",
  "description": "A webpack starter for angular 2",
  "scripts": {
    "profiling": "rimraf dist && webpack --json --profile --config webpack.aot.config.js > profile.json",
    "start": "webpack-dev-server --config webpack.jit.config.js --progress",
    "build:jit": "rimraf dist && webpack --config webpack.jit.config.js",
    "build:aot": "rimraf dist && webpack --config webpack.aot.config.js"
  },
  "license": "MIT",
  "dependencies": {
    "angular": "1.4.7",
    "@angular/upgrade": "4.3.4",
    "@angular/common": "4.3.4",
    "@angular/compiler": "4.3.4",
    "@angular/core": "4.3.4",
    "@angular/forms": "4.3.4",
    "@angular/http": "4.3.4",
    "@angular/platform-browser-dynamic": "4.3.4",
    "@angular/platform-browser": "4.3.4",
    "@angular/router": "4.3.4",
    "core-js": "2.5.0",
    "rxjs": "5.4.3",
    "zone.js": "0.8.16"
  },
  "devDependencies": {
    "@angular/compiler-cli": "4.3.4",
    "@ngtools/webpack": "1.6.0",
    "@types/node": "8.0.20",
    "css-loader": "0.28.4",
    "sass-loader": "6.0.6",
    "node-sass": "4.5.3",
    "extract-text-webpack-plugin": "3.0.0",
    "html-webpack-plugin": "2.30.1",
    "raw-loader": "0.5.1",
    "to-string-loader": "1.1.5",
    "rimraf": "2.6.1",
    "style-loader": "0.18.2",
    "typescript": "2.4.2",
    "webpack": "3.5.3",
    "webpack-dev-server": "2.7.1"
  }
}
