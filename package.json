{
  "name": "webpack-angular2",
  "version": "1.0.0",
  "description": "A webpack starter for angular 2",
  "scripts": {
    "profiling": "rimraf dist && webpack --json --profile --config webpack.aot.config.js > profile.json",
    "start": "webpack-dev-server --config webpack.jit.config.js --progress",
    "build:jit": "rimraf dist && webpack --config webpack.jit.config.js",
    "build:aot": "rimraf dist && webpack --config webpack.aot.config.js",
    "ngc": "ngc"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "4.0.0",
    "@angular/compiler": "4.0.0",
    "@angular/core": "4.0.0",
    "@angular/forms": "4.0.0",
    "@angular/http": "4.0.0",
    "@angular/platform-browser": "4.0.0",
    "@angular/platform-browser-dynamic": "4.0.0",
    "@angular/router": "4.0.0",
    "core-js": "2.4.1",
    "rxjs": "5.2.0",
    "zone.js": "0.8.5"
  },
  "devDependencies": {
    "@angular/compiler-cli": "4.0.0",
    "@angular/platform-server": "4.0.0",
    "@ngtools/webpack": "1.2.9",
    "@types/node": "7.0.13",
    "angular2-template-loader": "0.6.2",
    "awesome-typescript-loader": "3.1.2",
    "css-loader": "0.28.0",
    "sass-loader": "6.0.3",
    "node-sass": "4.5.2",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.8.5",
    "html-webpack-plugin": "2.28.0",
    "raw-loader": "0.5.1",
    "to-string-loader": "1.1.5",
    "rimraf": "2.6.1",
    "style-loader": "0.16.1",
    "typescript": "2.2.2",
    "webpack": "2.3.2",
    "webpack-dev-server": "2.4.4"
  }
}
